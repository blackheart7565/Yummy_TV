    <!--#region Gradient Color-->

    <LinearGradientBrush x:Key="GradientBrush1"
                         StartPoint="0,0"
                         EndPoint="1,1">
        <GradientStop Color="{StaticResource Color55}"
                      Offset="0" />
        <GradientStop Color="{StaticResource Color53}"
                      Offset="0.5" />
        <GradientStop Color="{StaticResource Color54}"
                      Offset="0.75" />
        <GradientStop Color="{StaticResource Color57}"
                      Offset="1" />
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="GradientBrush2" 
                         StartPoint="1,1" 
                         EndPoint="0,0">
        <GradientStop Color="{StaticResource Color54}" Offset="0" />
        <GradientStop Color="{StaticResource Color70}" Offset="0.5" />
        <GradientStop Color="{StaticResource Color57}" Offset="1" />
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="GradientBrush3" 
                         StartPoint="1,1" 
                         EndPoint="0,0">
        <GradientStop Color="{StaticResource Color56}" Offset="0" />
        <GradientStop Color="{StaticResource Color57}" Offset="0.5" />
        <GradientStop Color="{StaticResource Color71}" Offset="1" />
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="GradientBrush4"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="{StaticResource Color72}"
                      Offset="0" />
        <GradientStop Color="{StaticResource Color74}"
                      Offset="0.5" />
        <GradientStop Color="{StaticResource Color72}"
                      Offset="1" />
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="GradientBrush5"
                         StartPoint="0,0"
                         EndPoint="1,0">
        <LinearGradientBrush.GradientStops>
            <GradientStopCollection>
                <GradientStop Color="{StaticResource Color82}"
                              Offset="0" />
                <GradientStop Color="{StaticResource Color83}"
                              Offset="0.4" />
                <GradientStop Color="{StaticResource Color83}"
                              Offset="0.6" />
                <GradientStop Color="{StaticResource Color82}"
                              Offset="1" />
            </GradientStopCollection>
        </LinearGradientBrush.GradientStops>
    </LinearGradientBrush>

